<?xml version = "1.0" encoding = "utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout xmlns:android = "http://schemas.android.com/apk/res/android"
        xmlns:app = "http://schemas.android.com/apk/res-auto"
        xmlns:tools = "http://schemas.android.com/tools"
        android:id = "@+id/swipeRefresh"
        android:layout_width = "match_parent"
        android:layout_height = "match_parent"
        tools:context = ".HomeActivity">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="20dp"
                android:orientation="vertical"
                android:visibility="visible">
                <TextView
                    android:id="@+id/textView34"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:text="@string/Duelistheader"
                    android:textSize="25sp"
                    android:textColor="@color/colorPrimaryDark"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/tv_nodatafound"
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginTop="20dp"
                    android:textSize="@dimen/labelheadersise"
                    android:text="@string/norecordfound"
                    android:textColor="@color/blue"
                    />
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rl_weeklycases"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="visible"
                    android:layout_marginTop="@dimen/termprivacy"
                    />

            </LinearLayout>

</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

