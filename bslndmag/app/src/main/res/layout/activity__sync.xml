                android:layout_height="wrap_content"
                android:layout_weight="17"
                android:gravity="center"
                android:textStyle="bold"
                android:paddingLeft="@dimen/pv_pin_view_item_radius"
                android:text="@string/headermasterdatasync"
                android:textSize="@dimen/labelheadersise" />



            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_marginTop="@dimen/twentydp"

                >

                <LinearLayout
                    android:id="@+id/ll_childdata"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="20">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="17"
                        android:drawableLeft="@drawable/ic_sync_foreground"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/pv_pin_view_item_radius"
                        android:text="@string/headerchilddatasync" />

                    <TextView
                        android:id="@+id/tv_childdata"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:background="@drawable/textboxui"
                        android:gravity="center"
                        android:paddingLeft="@dimen/pv_pin_view_item_radius"
                        android:text="0"
                        android:textAlignment="center" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
